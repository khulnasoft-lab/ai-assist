name: "codegen"
backend: "python"
max_batch_size: 4

input [
  {
    name: "prompts"
    data_type: TYPE_STRING
    dims: [ 1 ]
  }
]

output [
  {
    name: "completions"
    data_type: TYPE_STRING
    dims: [ 1, 1 ]
  }
]

instance_group [
  {
      count: 1
      kind: {kind_group}
  }
]

parameters {
  key: "model_name"
  value: {
    string_value: "{model_name}"
  }
}
parameters {
  key: "model_path"
  value: {
    string_value: "{model_path}"
  }
}
parameters {
  key: "offload_folder"
  value: {
    string_value: "{offload_folder}"
  }
}